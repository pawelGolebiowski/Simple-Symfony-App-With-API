{% extends 'base.html.twig' %}

{% block content %}
    <h1>{{ form_title }}</h1>

    {{ form_start(form) }}
    {{ form_row(form.title) }}
    {{ form_row(form.text) }}
    {{ form_row(form.authors) }}
    <button type="submit" class="btn btn-primary">{{ form_button_label }}</button>
    {{ form_end(form) }}
{% endblock %}
